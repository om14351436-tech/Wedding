        <!-- Floating Buttons -->
        <div class="mt-10 flex gap-6 animate-fadeInUp" style="animation-delay: 1.4s">
            <button class="bg-[#25D366] text-white px-8 py-4 rounded-full text-[11px] font-black uppercase shadow-2xl hover:scale-105 transition-transform active:scale-95">WhatsApp Invite</button>
            <button class="bg-[#880e4f] text-white px-8 py-4 rounded-full text-[11px] font-black uppercase shadow-2xl hover:scale-105 transition-transform active:scale-95">Save Date</button>
        </div>

        <p class="mt-10 text-[9px] uppercase tracking-[0.6em] text-[#880e4f]/40 font-black mb-8">Patel Family • February 2026</p>
    </div>

    <script>
        // Data Initialization
        const ceremonies = [
            { name: "Matli Ceremony", time: "06:30 AM" },
            { name: "Ganesh Sthapana & Mandap Muhurat", time: "10:00 AM" },
            { name: "Pithi Ceremony", time: "11:00 AM" },
            { name: "Graha Shanti", time: "03:00 PM" }
        ];

        const regards = [
            "Mr. Dipikaben Jashabhai Patel",
            "Mr. jayshree ben Ashokbhai Patel",
            "Mrs. Sonalben Dishantbhai Patel",
            "Late Mr. Jayshree ben Pinakinbhai Patel",
            "Mrs. Apkesha Ben Bhaveshbhai Patel",
            "Mrs. Meghaben Bankimbhai Patel"
        ];

        const affectionately = [
            "Mr. Jashabhai Ishwarbhai Patel",
            "Mr. Dishantbhai Ratilal Patel",
            "Mr. Bankimbhai Ashokbhai Patel",
            "Mr. Mehul Ramanbhai Patel",
            "Mr. Bhaveshbhai Babubhai Patel",
            "Mr. Dhruvalbhai Pinakinbhai Patel",
            "Mr. Het Bhaveshbhai Patel",
            "The Patel Family",
            "(On behalf of the family of Late Vitthalbhai Behcharbhai Patel)"
        ];

        // Rendering functions
        function renderLists() {
            const cList = document.getElementById('ceremonies-list');
            ceremonies.forEach(c => {
                cList.innerHTML += `
                    <div class="flex justify-between items-end border-b border-[#f8bbd0] pb-2 group transition-all hover:bg-pink-50/50 p-1">
                        <span class="text-[#880e4f] font-black text-xs uppercase tracking-widest">${c.name}</span>
                        <div class="flex items-center gap-2 text-xs font-bold text-[#c5a059]">
                            <i data-lucide="clock" class="w-3 h-3"></i>
                            <span>${c.time}</span>
                        </div>
                    </div>
                `;
            });

            const rList = document.getElementById('regards-list');
            regards.forEach(r => {
                rList.innerHTML += `<li>${r}</li>`;
            });

            const aList = document.getElementById('affectionately-list');
            affectionately.forEach(a => {
                const isHeader = a.includes('Patel Family');
                aList.innerHTML += `<li class="${isHeader ? 'font-black text-[#880e4f] pt-1' : ''}">${a}</li>`;
            });

            const floralBg = document.getElementById('floral-bg');
            for(let i=0; i<24; i++) {
                floralBg.innerHTML += `<i data-lucide="flower-2" class="text-[#f06292] w-full h-auto animate-spin-slow" style="animation-delay: ${i * 0.5}s"></i>`;
            }

            lucide.createIcons();
        }

        // AI Concierge Logic
        async function handleAI(type) {
            const textEl = document.getElementById('ai-text');
            const loaderEl = document.getElementById('ai-loader');
            
            textEl.classList.add('hidden');
            loaderEl.classList.remove('hidden');

            let prompt = "";
            switch(type) {
                case 'blessing':
                    prompt = "Generate a traditional Hindu wedding blessing for Ashishkumar and Kwina mentioning Lord Ganesha, Mahakali, and Ranchhodji.";
                    break;
                case 'rituals':
                    prompt = "Explain the spiritual significance of Matli Ceremony and Pithi Ceremony in a Gujarati wedding concisely.";
                    break;
                case 'travel':
                    prompt = "Suggest 3 must-visit places in Vadodara and 2 local Gujarati snacks for wedding guests.";
                    break;
                case 'guestbook':
                    prompt = "Draft a warm message a guest can send to the couple Ashishkumar and Kwina wishing them prosperity.";
                    break;
            }

            try {
                const result = await callGemini(prompt);
                textEl.innerText = `"${result}"`;
            } catch (err) {
                textEl.innerText = "The ✨ AI Assistant is resting. Please try again soon!";
            } finally {
                loaderEl.classList.add('hidden');
                textEl.classList.remove('hidden');
            }
        }

        const apiKey = "";
        async function callGemini(prompt) {
            const url = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash-preview-09-2025:generateContent?key=${apiKey}`;
            const payload = {
                contents: [{ parts: [{ text: prompt }] }],
                systemInstruction: { 
                    parts: [{ text: "You are a warm, elegant Indian wedding concierge. Keep responses brief (under 50 words), celebratory, and respectful of Gujarati traditions." }] 
                }
            };

            const fetchWithRetry = async (retries = 5, delay = 1000) => {
                try {
                    const response = await fetch(url, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify(payload)
                    });
                    if (!response.ok) throw new Error('API Error');
                    const data = await response.json();
                    return data.candidates?.[0]?.content?.parts?.[0]?.text;
                } catch (err) {
                    if (retries > 0) {
                        await new Promise(res => setTimeout(res, delay));
                        return fetchWithRetry(retries - 1, delay * 2);
                    }
                    throw err;
                }
            };

            return await fetchWithRetry();
        }

        // Init
        window.onload = renderLists;
    </script>

</body></html>
